{
  "name": "fluxthis-todo",
  "version": "0.0.1",
  "description": "todo list built with node, express, mongo, and fluxthis",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev-server": "webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx --cache || true",
    "seed": "node server/tasks/seed-development.js",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickarora/fluxthis-todo.git"
  },
  "author": "Nick Arora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickarora/fluxthis-todo/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "homepage": "https://github.com/nickarora/fluxthis-todo#readme",
  "dependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.8.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "fluxthis": "^2.7.1",
    "mongoose": "^4.4.14",
    "mongoose-timestamp": "^0.5.0",
    "react": "^15.0.2",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "webpack-dev-server": "^1.14.1"
  }
}
